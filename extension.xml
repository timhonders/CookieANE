<extension xmlns="http://ns.adobe.com/air/extension/3.1">
    
    <id>com.kega.extensions.Cookie</id>
    
    <versionNumber>1.0</versionNumber>
    
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment> 
                <nativeLibrary>libCookieANE.a</nativeLibrary> 
                <initializer>ExtentionInitializer</initializer>
                <finalizer>ExtentionFinalizer</finalizer>
            </applicationDeployment> 
        </platform>
		
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libCookieANE.a</nativeLibrary> 
                <initializer>ExtentionInitializer</initializer>
                <finalizer>ExtentionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		
		<platform name="Android-ARM"> 
            <applicationDeployment> 
                <nativeLibrary>cookie.jar</nativeLibrary> 
                <initializer>com.kega.extensions.Cookie.Extension</initializer> 
                <finalizer>com.kega.extensions.Cookie.Extension</finalizer> 
            </applicationDeployment> 
        </platform> 
		
		<platform name="default"> 
            <applicationDeployment/> 
        </platform> 

    </platforms>
    
</extension>